<div class="row span8" style="margin-left:0px;">
<table class="table table-hover">
  <tr>
    <th>Name</th>
    <th>Wins</th>
    <th>Losses</th>
    <th>Rating</th>
  </tr>

  <% @players.each do |player| %>
    <tr>
      <td><%= link_to player.name, :controller => 'players', :action => 'show', :id => player.id %></td>
      <td><%= player.wins %></td>
      <td><%= player.losses %></td>
      <td><%= player.rating %></td>
    </tr>
  <% end %>
</table>
</div>
